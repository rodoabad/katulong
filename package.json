{
  "name": "katulong",
  "description": "A collection of scripts to be used in an npm package to make tooling easier",
  "keywords": [
    "katulong",
    "build",
    "task",
    "runner",
    "coverage",
    "lint",
    "preview",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rodoabad/katulong.git"
  },
  "author": "Rodo Abad <rodoabad@gmail.com> (https://github.com/rodoabad)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rodoabad/katulong/issues"
  },
  "homepage": "https://github.com/rodoabad/katulong#readme",
  "engines": {
    "node": ">=5.x"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test",
      "commit-msg": "validate-commit-msg"
    }
  },
  "dependencies": {
    "autoprefixer": "6.4.0",
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "code": "3.0.2",
    "css-loader": "0.23.1",
    "eslint": "3.3.1",
    "file-loader": "0.9.0",
    "isparta": "3.5.3",
    "istanbul": "0.4.4",
    "lost": "7.0.4",
    "mocha": "3.0.2",
    "mocha-clean": "1.0.0",
    "node-sass": "3.8.0",
    "postcss": "5.1.2",
    "postcss-loader": "0.10.0",
    "react": "15.3.0",
    "react-hot-loader": "1.3.0",
    "sass-loader": "4.0.0",
    "shelljs": "0.7.3",
    "sinon": "1.17.5",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "yargs": "5.0.0"
  },
  "devDependencies": {
    "eslint-config-rodoabad": "1.8.0",
    "ghooks": "1.3.2",
    "semantic-release": "4.3.5",
    "validate-commit-msg": "2.7.0"
  },
  "bin": {
    "katulong": "bin/katulong.js"
  },
  "main": "index.js",
  "scripts": {
    "pkg:lint": "node bin/katulong.js lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "npm run pkg:lint"
  }
}
